#include <stdio.h>
#include "main.h"
/**
 *_print_sign - entry point
 *@c: a letter
 *
 *Description: checks if a letter is in uppercase
 *Return: either 1 or 0
 */
int print_sign(int n)
{
if (n > 0)
{
printf("+");
return (1);
}
else if (n == 0)
{
printf("0");
return (0);
}
else
{
printf("-");
return (-1);
};
return (0);
}
